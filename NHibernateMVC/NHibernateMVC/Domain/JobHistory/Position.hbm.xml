<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="NHibernateMVC" namespace="NHibernateMVC.Domain.JobHistory" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Position" table="Position" lazy="true" >
    <id name="PositionId" column="PositionId">
      <generator class="identity" />
    </id>
    <property name="Name">
      <column name="Name" sql-type="varchar" not-null="true" />
    </property>
    <property name="Code">
      <column name="Code" sql-type="varchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>